<!-- > error -->
{{#> 'layouts/main'}}
{{#*inline 'document-title' this}}{{this.dirname}}{{/inline}}
{{#*inline 'content' this}}
{{#if this.error}}
<div class="row">
  <div class="col s12">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Error</span>
        <pre><code>{{this.error}}</code></pre>
      </div>
    </div>
  </div>
</div>
{{/if}}
{{/inline}}
{{/layouts/main}}
